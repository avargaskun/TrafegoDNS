  "dependencies": {
    "axios": "^1.8.3",
    "@aws-sdk/client-route-53": "^3.540.0",
    "dockerode": "^4.0.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "express-basic-auth": "^1.2.1",
    "nodemon": "^3.0.1",
    "cross-env": "^7.0.3"
  }
}