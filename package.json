{
  "name": "TrafegoDNS",
  "version": "1.8.1",
  "description": "Automatically manage DNS records based on Traefik routing configuration or Docker labels",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:webui": "cross-env ENABLE_WEB_UI=true node src/app.js",
